{
  "manifest_version": 3,
  "name": "Opa&SGP Extension",
  "version": "1.37",
  "description": "Extens√£o para facilitar e agilizar o atendimento no Opa! Suite e SGP",
  "permissions": ["storage", "scripting", "tabs"],
  "action": {
    "default_popup": "settings.html"
  },
   "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "content_scripts": [
    {
      "matches": ["https://atendimento.fibranetbrasil.com.br/*"],
      "js": [
        "sgpIntegration.js",
        "fastTransfer.js",
        "betterNotifications.js"
      ]
    }
  ]
}